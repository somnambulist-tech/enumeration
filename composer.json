    "keywords": ["enum", "enumeration", "set", "class", "multiton", "type"],
    "homepage": "https://github.com/somnambulist/enumeration",
    "license": "MIT",
    "authors": [
        {
            "name": "David Redfern",
            "email": "info@somnambulist.tech",
            "homepage": "https://somnambulist.tech/"
        },
        {
            "name": "Erin Millard",
            "email": "ezzatron@gmail.com",
            "homepage": "http://ezzatron.com/"
        }
    ],
    "require": {
